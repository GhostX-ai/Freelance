@model List<FreelanceV2.Models.UserInfo>

<h2 class="text-center">Заявки программистов</h2>
<div>
    @foreach (var x in Model)
    {
        <div class="card" style="width: 18rem;">
            <img src="@x.ImagePath" class="card-img-top">
            <div class="card-body">
                <h5 class="card-title text-center">@x.FirstName</h5>
                <p class="card-text">@x.BindText</p>
                <i class="card-text">Ранг:@x.Rang</i><br>
                <i class="card-text">Опыт:@x.Skills</i>
                <div class="card-footer">
                    <a href="#" class="btn btn-primary">Принять заявку</a>
                    <a href="#" class="btn btn-info">Подробнее</a>
                </div>
            </div>
        </div>
    }
</div>